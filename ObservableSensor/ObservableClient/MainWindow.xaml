<Window x:Class="MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <Style TargetType="Button">
            <Setter Property="Width" Value="150"></Setter>
            <Setter Property="Margin" Value="5"></Setter>
        </Style>
    </Window.Resources>
    <StackPanel>
        <Button Name="FileWatcherButton" Content="FileWatcher"></Button>
        <Button Name="Start" Content="Start"></Button>
        <Button Name="FilterLowValue" Content="Filter Low Values"></Button>
        <Button Name="FilterFirstType" Content="Filter First Type"></Button>
        <Button Name="QueryAny" Content="Test Any"></Button>
        <Button Name="Heartbeat" Content="Test Throttle"></Button>
        <Button Name="MouseMoveSamples" Content="Mouse Move" ></Button>
        <Button Name="Stop" Content="Stop"></Button>
        <ListBox Name="FilteredList" ></ListBox>
    </StackPanel>
</Window>
