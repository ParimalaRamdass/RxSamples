<navigation:Page x:Class="SilverlightApplication1.Views.EnumObs" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
           d:DesignWidth="640" d:DesignHeight="480"
           Title="EnumObs Page">
    <StackPanel>
        <Button x:Name="EnumButton" Content="AsEnumerable" Width="100" Margin="5" Click="Enumerable_Click"></Button>
        <Button x:Name="ObsButton" Content="AsObservable" Width="100" Margin="5" Click="Observable_Click"></Button>
        <TextBlock  x:Name="TotalTime" Text="TotalTime: "></TextBlock>
        <ListBox x:Name="Results" ></ListBox>
    </StackPanel>
</navigation:Page>
